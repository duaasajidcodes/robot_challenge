# Test Environment Configuration for Robot Challenge
# This file contains test-specific environment variables

# Table dimensions for testing
ROBOT_TABLE_WIDTH=5
ROBOT_TABLE_HEIGHT=5

# Test mode settings
ROBOT_TEST_MODE=true
ROBOT_DEBUG_MODE=false

# Output settings for testing
